<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
<!--         <item name="android:background">#d1e5be</item> -->
    </style>
	<style name="CustomButton" parent="android:style/Widget.Button">
  			<item name="android:gravity">center</item>
  			<item name="android:background">@drawable/rounded_buttons</item>
  			<item name="android:drawableRight">@drawable/button</item>
  			<item name="android:textColor">@drawable/button_pressed_text_colour</item>
  	</style>
  	<style name="SubmitButton" parent="android:style/Widget.Button">
  			<item name="android:gravity">center</item>
  			<item name="android:background">@drawable/submit_buttons</item>
  			<item name="android:textColor">@drawable/button_pressed_text_colour</item>
  	</style>
  	<style name="DefaultButtons" parent="android:style/Widget.Button">
  			<item name="android:gravity">center</item>
  			<item name="android:background">@drawable/default_buttons</item>
  			<item name="android:textColor">@drawable/button_pressed_text_colour</item>
  	</style>
  	<style name="TitleLabel" parent="android:style/Widget.TextView">
  	    	<item name="android:gravity">center</item>
  	    	<item name="android:textSize">30sp</item>
  	    	<item name="android:background">@drawable/title_label</item>:
  	</style>
</resources>