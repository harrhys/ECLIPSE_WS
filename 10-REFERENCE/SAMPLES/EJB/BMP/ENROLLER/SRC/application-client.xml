<!DOCTYPE application-client PUBLIC '-//Sun Microsystems, Inc.//DTD J2EE Application Client 1.3//EN' 'http://java.sun.com/dtd/application-client_1_3.dtd'>

<application-client>
  <display-name>EnrollerClient</display-name>
  <ejb-ref>
    <ejb-ref-name>ejb/SimpleCourse</ejb-ref-name>
    <ejb-ref-type>Entity</ejb-ref-type>
    <home>samples.ejb.bmp.enroller.ejb.CourseHome</home>
    <remote>samples.ejb.bmp.enroller.ejb.Course</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/SimpleStudent</ejb-ref-name>
    <ejb-ref-type>Entity</ejb-ref-type>
    <home>samples.ejb.bmp.enroller.ejb.StudentHome</home>
    <remote>samples.ejb.bmp.enroller.ejb.Student</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/SimpleEnroller</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>samples.ejb.bmp.enroller.ejb.EnrollerHome</home>
    <remote>samples.ejb.bmp.enroller.ejb.Enroller</remote>
  </ejb-ref>
</application-client>

