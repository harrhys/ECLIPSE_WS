<?xml version="1.0" encoding="UTF-8"?> 

<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
    "http://java.sun.com/dtd/web-app_2_3.dtd"> 

<web-app> 
   <display-name>SupplierApplication</display-name> 
   <description>Coffee Supplier Application</description> 
   <servlet> 
      <servlet-name>JAXMPriceListEndpoint</servlet-name> 
      <servlet-class>com.sun.cb.PriceListServlet</servlet-class> 
	  <load-on-startup>1</load-on-startup>
   </servlet> 
   <servlet> 
      <servlet-name>JAXMOrderConfirmationEndpoint</servlet-name> 
      <servlet-class>com.sun.cb.ConfirmationServlet</servlet-class> 
	  <load-on-startup>2</load-on-startup>
   </servlet> 
   <servlet-mapping> 
      <servlet-name>JAXMPriceListEndpoint</servlet-name> 
      <url-pattern>/getPriceList</url-pattern> 
   </servlet-mapping> 
   <servlet-mapping> 
      <servlet-name>JAXMOrderConfirmationEndpoint</servlet-name> 
      <url-pattern>/orderCoffee</url-pattern> 
   </servlet-mapping> 
   <session-config> 
      <session-timeout>60</session-timeout> 
   </session-config> 
</web-app>

