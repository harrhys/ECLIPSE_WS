<sun-web-app>
 <session-config>
      <session-manager/>
	    </session-config>

  <cache enabled="true" timeout-in-seconds="300" >
    <cache-mapping>
	  <servlet-name>ServCache</servlet-name>
	  <key-field name="inputtext" scope="request.parameter"/>
	  <constraint-field name="inputtext" scope="request.parameter">
	  	<value>one</value>
		<value>two</value>
	  </constraint-field>
	</cache-mapping>
  </cache>
</sun-web-app>

