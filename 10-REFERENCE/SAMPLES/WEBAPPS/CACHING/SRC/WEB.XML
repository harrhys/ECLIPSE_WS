<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>

<web-app>
  <display-name>webapps-caching</display-name>
  <distributable></distributable>
  <servlet>
    <servlet-name>ServCache</servlet-name>
    <servlet-class>samples.webapps.caching.ServCache</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServCache</servlet-name>
    <url-pattern>/ServCache</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>

</web-app>

