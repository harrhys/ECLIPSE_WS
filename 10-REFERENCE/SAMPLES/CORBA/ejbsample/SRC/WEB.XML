<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
  <display-name>CorbaServlet</display-name>
  <distributable></distributable>
  <servlet>
    <servlet-name>CorbaServlet</servlet-name>
    <display-name>CorbaServlet</display-name>
    <description>Default configuration created for servlet.</description>
    <servlet-class>samples.corba.ejbsample.servlet.CorbaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CorbaServlet</servlet-name>
    <url-pattern>/CorbaServlet</url-pattern>
  </servlet-mapping>
  <ejb-ref>
    <ejb-ref-name>ejb/TheCorbaClient</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>samples.corba.ejbsample.ejb.CorbaClientHome</home>
    <remote>samples.corba.ejbsample.ejb.CorbaClient</remote>
  </ejb-ref>
</web-app>
