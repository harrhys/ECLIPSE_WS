<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">

<web-app>
	<display-name>FubarServlet</display-name>
	<description>
		FubarServlet
	</description>
	<context-param>
		<param-name>FubarServlet</param-name>
		<param-value>FubarServlet</param-value>
		<description>
			FubarServlet
		</description>
	</context-param>
	<servlet>
		<servlet-name>FubarServlet</servlet-name>
		<description>
			FubarServlet
		</description>
		<servlet-class>
				com.weblogic.sample.webapp.FubarServlet
		</servlet-class>
		<load-on-startup>5</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>
			FubarServlet
		</servlet-name>
		<url-pattern>
			/FubarServlet
		</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>
			/FubarServlet
		</welcome-file>
	</welcome-file-list>
</web-app>
