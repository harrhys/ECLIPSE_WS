<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:sca="http://xmlns.oracle.com/weblogic/weblogic-sca"
       xmlns:wlsb="http://xmlns.oracle.com/weblogic/weblogic-sca-binding"
       xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://xmlns.oracle.com/weblogic/weblogic-sca
                           http://xmlns.oracle.com/weblogic/weblogic-sca/1.0/weblogic-sca.xsd
                           http://xmlns.oracle.com/weblogic/weblogic-sca-binding
                           http://xmlns.oracle.com/weblogic/weblogic-sca-binding/1.0/weblogic-sca-binding.xsd">

	<sca:service name="GetPriceSvc_EJB30"
		type="com.oracle.enterprisetest.jsca.getprc.svc.ejb30.IGetPrice"
		target="GetPriceSvc_EJB30_Target">
		<wlsb:binding.ejb uri="GetPriceSvc_EJB30_JNDI" remote="true" />
	</sca:service>

	<sca:service name="GetPriceSvc_WS"
		type="com.oracle.enterprisetest.jsca.getprc.svc.ws.IGetPrice"
		target="GetPriceSvc_WS_Target">
		<wlsb:binding.ws 
			name="GetPriceSvc_WS_Name" port="GetPriceSvc_WS_Port"
			uri="/GetPriceSvc_WS_URI" />
	</sca:service>

	<sca:reference name="GetPriceRef_EJB20"
		type="com.oracle.enterprisetest.jsca.getprc.ref.ejb20.IGetPrice">
		<wlsb:binding.ejb uri="GetPriceRef_EJB20_JNDI" />
	</sca:reference>

	<sca:reference name="GetPriceRef_EJB30"
		type="com.oracle.enterprisetest.jsca.getprc.ref.ejb30.IGetPrice">
		<wlsb:binding.ejb uri="GetPriceRef_EJB30_JNDI#com.oracle.enterprisetest.jsca.getprc.ejb30.IGetPriceRemote" />
	</sca:reference>

	<sca:reference name="GetPriceRef_WS"
		type="com.oracle.enterprisetest.jsca.getprc.ref.ws.IGetPrice">
		<wlsb:binding.ws
			location="http://{jscaApp_GetTotalPrice_WebService_HostPort}/JSca_GetTotPrice_CTX/GetPriceSvc_WS_URI"
			port="http://ws.svc.getprc.jsca.enterprisetest.oracle.com#wsdl.endpoint(GetPriceSvc_WS_Name/GetPriceSvc_WS_Port)" />
	</sca:reference>

	<sca:reference name="GetPriceRef_WS_Ext"
		type="com.oracle.enterprisetest.jsca.getprc.ref.ws.IGetPrice">
		<wlsb:binding.ws
			location="http://{reference_GetTotalPrice_WebService_HostPort}/GetPriceRef_WS_CTX/GetPriceRef_WS_URI"
			port="http://jsca.oracle.com#wsdl.endpoint(GetPriceRef_WS_Name/GetPriceRef_WS_Port)" />
	</sca:reference>

	<bean id="GetPriceSvc_EJB30_Target"
		class="com.oracle.enterprisetest.jsca.getprc.beans.GetPriceImpl_EJB30">
		<property name="bean1" ref="bean1" />
		<property name="bean2" ref="bean2" />
	</bean>

	<bean id="GetPriceSvc_WS_Target"
		class="com.oracle.enterprisetest.jsca.getprc.beans.GetPriceImpl_WS">
	</bean>

	<bean id="bean1"
		class="com.oracle.enterprisetest.jsca.getprc.beans.Bean1">
	</bean>

	<bean id="bean2"
		class="com.oracle.enterprisetest.jsca.getprc.beans.Bean2">
		<property name="bean21" ref="bean21" />
		<property name="bean22" ref="bean22" />
	</bean>

	<bean id="bean21"
		class="com.oracle.enterprisetest.jsca.getprc.beans.Bean21">
	</bean>

	<bean id="bean22"
		class="com.oracle.enterprisetest.jsca.getprc.beans.Bean22">
		<property name="bean221" ref="bean221" />
		<property name="bean222" ref="bean222" />
	</bean>

	<bean id="bean221"
		class="com.oracle.enterprisetest.jsca.getprc.beans.Bean221">
	</bean>

	<bean id="bean222"
		class="com.oracle.enterprisetest.jsca.getprc.beans.Bean222">
		<property name="getPriceEJB20Ref" ref="GetPriceRef_EJB20" />
		<property name="getPriceEJB30Ref" ref="GetPriceRef_EJB30" />
		<property name="getPriceWSRef" ref="GetPriceRef_WS" />
	</bean>

</beans>
