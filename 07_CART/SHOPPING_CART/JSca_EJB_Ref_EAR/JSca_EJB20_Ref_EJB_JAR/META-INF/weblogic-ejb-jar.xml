<?xml version="1.0"?>
<weblogic-ejb-jar
  xmlns="http://www.oracle.com/technology/weblogic/weblogic-ejb-jar" xmlns:j2ee="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.oracle.com/technology/weblogic/weblogic-ejb-jar http://www.oracle.com/technology/weblogic/weblogic-ejb-jar/1.1/weblogic-ejb-jar.xsd">
	<weblogic-enterprise-bean>
		<ejb-name>GetPriceBean20</ejb-name>
		<stateless-session-descriptor>
			<stateless-clustering>
				<stateless-bean-is-clusterable>
					true
				</stateless-bean-is-clusterable>
			</stateless-clustering>
		</stateless-session-descriptor>
		<transaction-descriptor>
			<trans-timeout-seconds>300</trans-timeout-seconds>
		</transaction-descriptor>
		<enable-call-by-reference>True</enable-call-by-reference>
		<jndi-name>GetPriceRef_EJB20_JNDI</jndi-name>
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>GetQuantityBean20</ejb-name>
		<stateless-session-descriptor>
			<stateless-clustering>
				<stateless-bean-is-clusterable>
					true
				</stateless-bean-is-clusterable>
			</stateless-clustering>
		</stateless-session-descriptor>
		<transaction-descriptor>
			<trans-timeout-seconds>300</trans-timeout-seconds>
		</transaction-descriptor>
		<enable-call-by-reference>True</enable-call-by-reference>
		<jndi-name>GetQuantityRef_EJB20_JNDI</jndi-name>
	</weblogic-enterprise-bean>
</weblogic-ejb-jar>
