<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:sca="http://xmlns.oracle.com/weblogic/weblogic-sca"
       xmlns:wlsb="http://xmlns.oracle.com/weblogic/weblogic-sca-binding"
       xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://xmlns.oracle.com/weblogic/weblogic-sca
                           http://xmlns.oracle.com/weblogic/weblogic-sca/1.0/weblogic-sca.xsd
                           http://xmlns.oracle.com/weblogic/weblogic-sca-binding
                           http://xmlns.oracle.com/weblogic/weblogic-sca-binding/1.0/weblogic-sca-binding.xsd">

	<sca:service name="GetQuantitySvc_EJB30"
		type="com.oracle.enterprisetest.jsca.getqty.svc.ejb30.IGetQuantity"
		target="GetQuantitySvc_EJB30_Target">
		<wlsb:binding.ejb uri="GetQuantitySvc_EJB30_JNDI" remote="true" />
	</sca:service>

	<sca:service name="GetQuantitySvc_WS"
		type="com.oracle.enterprisetest.jsca.getqty.svc.ws.IGetQuantity"
		target="GetQuantitySvc_WS_Target">
		<binding.ws xmlns="http://xmlns.oracle.com/weblogic/weblogic-sca-binding"
			name="GetQuantitySvc_WS_Name" port="GetQuantitySvc_WS_Port"
			uri="/GetQuantitySvc_WS_URI" />
	</sca:service>

	<sca:reference name="GetQuantityRef_EJB20"
		type="com.oracle.enterprisetest.jsca.getqty.ref.ejb20.IGetQuantity">
		<wlsb:binding.ejb uri="GetQuantityRef_EJB20_JNDI" />
	</sca:reference>

	<sca:reference name="GetQuantityRef_EJB30_Ext"
		type="com.oracle.enterprisetest.jsca.getqty.ref.ejb30.IGetQuantity">
		<wlsb:binding.ejb uri="GetQuantityRef_EJB30_JNDI#com.oracle.enterprisetest.jsca.getqty.ejb30.IGetQuantityRemote" />
	</sca:reference>

	<sca:reference name="GetQuantityRef_EJB30"
		type="com.oracle.enterprisetest.jsca.getqty.ref.ejb30.IGetQuantity">
		<wlsb:binding.ejb uri="GetQuantitySvc_EJB30_JNDI" />
	</sca:reference>

	<sca:reference name="GetQuantityRef_WS_Internal"
		type="com.oracle.enterprisetest.jsca.getqty.ref.ws.IGetQuantity">
		<binding.ws xmlns="http://xmlns.oracle.com/weblogic/weblogic-sca-binding"
			location="http://{jscaApp_GetTotalQuantity_WebService_HostPort}/JSca_GetTotQuantity_CTX/GetQuantitySvc_WS_URI"
			port="http://ws.svc.getqty.jsca.enterprisetest.oracle.com#wsdl.endpoint(GetQuantitySvc_WS_Name/GetQuantitySvc_WS_Port)" />
	</sca:reference>

	<sca:reference name="GetQuantityRef_WS"
		type="com.oracle.enterprisetest.jsca.getqty.ref.ws.IGetQuantity">
		<binding.ws xmlns="http://xmlns.oracle.com/weblogic/weblogic-sca-binding"
			location="http://{reference_GetTotalQuantity_WebService_HostPort}/GetQuantityRef_WS_CTX/GetQuantityRef_WS_URI"
			port="http://jsca.oracle.com#wsdl.endpoint(GetQuantityRef_WS_Name/GetQuantityRef_WS_Port)" />
	</sca:reference>

	<bean id="GetQuantitySvc_WS_Target"
		class="com.oracle.enterprisetest.jsca.getqty.beans.GetQuantityImpl_WS">
		<property name="bean1" ref="bean1" />
		<property name="bean2" ref="bean2" />
	</bean>

	<bean id="GetQuantitySvc_EJB30_Target"
		class="com.oracle.enterprisetest.jsca.getqty.beans.GetQuantityImpl_EJB30">
	</bean>

	<bean id="bean1"
		class="com.oracle.enterprisetest.jsca.getqty.beans.Bean1">
	</bean>

	<bean id="bean2"
		class="com.oracle.enterprisetest.jsca.getqty.beans.Bean2">
		<property name="bean21" ref="bean21" />
		<property name="bean22" ref="bean22" />
	</bean>

	<bean id="bean21"
		class="com.oracle.enterprisetest.jsca.getqty.beans.Bean21">
	</bean>

	<bean id="bean22"
		class="com.oracle.enterprisetest.jsca.getqty.beans.Bean22">
		<property name="bean221" ref="bean221" />
		<property name="bean222" ref="bean222" />
	</bean>

	<bean id="bean221"
		class="com.oracle.enterprisetest.jsca.getqty.beans.Bean221">
	</bean>

	<bean id="bean222"
		class="com.oracle.enterprisetest.jsca.getqty.beans.Bean222">
		<property name="getQuantityEJB20Ref" ref="GetQuantityRef_EJB20" />
		<property name="getQuantityEJB30Ref" ref="GetQuantityRef_EJB30" />
		<property name="getQuantityWSRef" ref="GetQuantityRef_WS" />
	</bean>

</beans>
