		<version>1.0</version>
	</parent>

	<artifactId>JERSEY_RS_IMPL</artifactId>
	<name>JERSEY_RS_IMPL</name>
	<packaging>war</packaging>

	<dependencies>
		<dependency>
			<groupId>com.sun.jersey</groupId>
			<artifactId>jersey-server</artifactId>
		</dependency>
		<dependency>
