<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DocEmpImplService WSDL service -->

  <service name="DocEmpImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://emp.test.com"/>
      <parameter name="wsdlServiceElement" value="DocEmpImplService"/>
      <parameter name="schemaQualified" value="http://dto.emp.test.com,http://emp.test.com"/>
      <parameter name="wsdlServicePort" value="DocEmpImpl"/>
      <parameter name="className" value="com.test.emp.DocEmpImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="DocEmpImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sayHello" qname="operNS:sayHello" xmlns:operNS="http://emp.test.com" returnQName="retNS:sayHelloReturn" xmlns:retNS="http://emp.test.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://emp.test.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="addEmployee" qname="operNS:addEmployee" xmlns:operNS="http://emp.test.com" soapAction="" >
        <parameter qname="pns:emp" xmlns:pns="http://emp.test.com" type="tns:EmpTO" xmlns:tns="http://dto.emp.test.com"/>
      </operation>
      <operation name="getEmployees" qname="operNS:getEmployees" xmlns:operNS="http://emp.test.com" returnQName="retNS:getEmployeesReturn" xmlns:retNS="http://emp.test.com" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://emp.test.com" returnItemQName="tns:item" xmlns:tns="http://emp.test.com" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getEmployees addEmployee sayHello"/>

      <typeMapping
        xmlns:ns="http://emp.test.com"
        qname="ns:>getEmployeesResponse"
        type="java:com.test.emp.GetEmployeesResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://emp.test.com"
        qname="ns:>addEmployeeResponse"
        type="java:com.test.emp.AddEmployeeResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://emp.test.com"
        qname="ns:>addEmployee"
        type="java:com.test.emp.AddEmployee"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dto.emp.test.com"
        qname="ns:EmpTO"
        type="java:com.test.emp.dto.EmpTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://emp.test.com"
        qname="ns:>sayHelloResponse"
        type="java:com.test.emp.SayHelloResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://emp.test.com"
        qname="ns:>getEmployees"
        type="java:com.test.emp.GetEmployees"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://emp.test.com"
        qname="ns:>sayHello"
        type="java:com.test.emp.SayHello"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://emp.test.com"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
  </service>
</deployment>
