<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from MyRESTServiceService WSDL service -->

  <service name="MyRESTService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://rs.farbig.com"/>
      <parameter name="wsdlServiceElement" value="MyRESTServiceService"/>
      <parameter name="schemaQualified" value="http://rs.farbig.com"/>
      <parameter name="wsdlServicePort" value="MyRESTService"/>
      <parameter name="className" value="com.farbig.rs.MyRESTServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="MyRESTService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="resourceMethodPUT" qname="operNS:resourceMethodPUT" xmlns:operNS="http://rs.farbig.com" soapAction="" >
        <parameter qname="pns:content" xmlns:pns="http://rs.farbig.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="resourceMethodGET" qname="operNS:resourceMethodGET" xmlns:operNS="http://rs.farbig.com" returnQName="retNS:resourceMethodGETReturn" xmlns:retNS="http://rs.farbig.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="resourceMethodGET resourceMethodPUT"/>

  </service>
</deployment>
